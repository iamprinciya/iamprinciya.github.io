        image: { src: '~/assets/images/websc-2022.jpeg', alt: 'Web summer camp 2022, frontend to serverless' },
        year: 2022,
        type: 'Web Summer Camp, Croatia',
        links: [{
          Link: 'https://github.com/princiya/websc-2022'
        }]
      },
      {
        title: 'Keynote: Reset and Recharge',
